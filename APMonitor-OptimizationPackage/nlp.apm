!Example nonlinear programming problem
!
!minimize    f(x)
!subject to  h(x) > 0
!            g(x) = 0
!
! In this case:
! f(x) = x1*x4 *(x1+x2+x3) +x3
! h(x) = x1*x2*x3*x4 -25
! g(x) = x1^2 + x2^2 +x3^2 + x4^2 -40
!
Model
	Variables
		x1 = 1 , >= 1 , <= 5
		x2 = 5 , >= 1 , <= 5
		x3 = 5 , >= 1 , <= 5
		x4 = 1 , >= 1 , <= 5
	End Variables

	Equations
		minimize x1*x4*(x1+x2+x3) + x3

		x1*x2*x3*x4 > 25
		x1^2 + x2^2 + x3^2 + x4^2 = 40
	End Equations
End Model